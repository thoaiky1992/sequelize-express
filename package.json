{
  "name": "sequelize-postgres-nodejs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build": "npm run clean && babel ./src -d ./build/src",
    "production": "npm run build && node ./build/src/app.js",
    "dev": "nodemon --exec ./node_modules/.bin/babel-node ./src/app.js",
    "lint": "eslint --fix ./src/**/*.js",
    "docker:build": "docker build -t sequelize_express .",
    "docker:run": "docker container run -d  --name sequelize_express  -v \"$(pwd)\"/public:/user/src/app/public --restart on-failure -it -p 4000:4000 --network=app-network sequelize_express",
    "docker:remove": "docker container rm sequelize_express",
    "docker:stop": "docker container stop sequelize_express",
    "docker:start": "docker container start sequelize_express",
    "docker:restart": "docker container restart sequelize_express",
    "docker:deploy": "npm run docker:build && npm run docker:stop || true && npm run docker:remove || true && npm run docker:run && docker rmi $(sudo docker images -f \"dangling=true\" -q)"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "joi": "^17.6.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "redis": "^4.5.1",
    "sequelize": "^6.24.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/node": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "@babel/runtime": "^7.19.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.25.0"
  }
}
